                                behavior:ZoomByWheelBehavior.IsEnabled="True"
                                behavior:MoveByDragBehavior.IsEnabled="True"
                                behavior:ResetRangeByDoubleClickBehavior.IsEnabled="True"
                                Grid.Column="2" Grid.Row="0">
                            <ContentControl Content="{Binding}"/>
                        </Border>
                        <chart:VerticalAxisControl VerticalAxis="{Binding Path=(core:ChartBaseControl.VerticalAxis)}"
                                                   FlippedY="{Binding Path=(core:ChartBaseControl.FlippedY)}"
                                                   HorizontalAxis="{x:Null}"
                                                   Margin="0,0,0,1"
                                                   Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="0"/>
                        <chart:HorizontalAxisControl HorizontalAxis="{Binding Path=(core:ChartBaseControl.HorizontalAxis)}"
                                                     FlippedX="{Binding Path=(core:ChartBaseControl.FlippedX)}"
                                                     VerticalAxis="{x:Null}"
                                                     Margin="1,0,0,0"
                                                     Grid.Column="2" Grid.Row="1" Grid.RowSpan="2"/>
