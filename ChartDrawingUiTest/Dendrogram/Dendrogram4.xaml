<Page x:Class="ChartDrawingUiTest.Dendrogram.Dendrogram4"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ChartDrawingUiTest.Dendrogram"
      xmlns:chart="clr-namespace:CompMs.Graphics.Base;assembly=ChartDrawing"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Dendrogram4">
    <Page.DataContext>
        <local:DendrogramVM3/>
    </Page.DataContext>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <chart:DefaultControl
            DrawingChart="{Binding XDrawingDendrogram}"
            Margin="5"
            Grid.Column="0"/>
        <chart:DefaultControl
            DrawingChart="{Binding YDrawingDendrogram}"
            Margin="5"
            Grid.Column="1"/>
    </Grid>
</Page>
