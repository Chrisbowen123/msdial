<Page x:Class="ChartDrawingUiTest.LineChart.LineChart1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ChartDrawingUiTest.LineChart"
      xmlns:chart="clr-namespace:CompMs.Graphics.Base;assembly=ChartDrawing"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LineChart1">
    <Page.DataContext>
        <local:LineChartVM/>
    </Page.DataContext>
    <Grid>
        <chart:DefaultControl DrawingChart="{Binding DrawingLineChart}"
                              Margin="5"/>
    </Grid>
</Page>
