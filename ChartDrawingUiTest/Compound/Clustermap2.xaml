<Page x:Class="ChartDrawingUiTest.Compound.Clustermap2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ChartDrawingUiTest.Compound"
      xmlns:compound="clr-namespace:CompMs.Graphics.Compound;assembly=ChartDrawing"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Clustermap2">
    <Page.DataContext>
        <local:ClusterVM2/>
    </Page.DataContext>
    <Grid>
        <compound:Clustermap
            DataContext="{Binding}"
            XDendrogram="{Binding XDendrogram}"
            YDendrogram="{Binding YDendrogram}"
            DataMatrix="{Binding DataMatrix}"
            XLabels="{Binding XLabels}"
            YLabels="{Binding YLabels}"
            Margin="10"/>
    </Grid>
</Page>
