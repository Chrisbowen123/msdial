<Page x:Class="ChartDrawingUiTest.Compound.Clustermap1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ChartDrawingUiTest.Compound"
      xmlns:compound="clr-namespace:CompMs.Graphics.Compound;assembly=ChartDrawing"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Clustermap1">
    <Page.DataContext>
        <local:ClusterVM1
            YLabelLimit="10"/>
    </Page.DataContext>
    <Grid>
        <compound:Clustermap
            DataContext="{Binding}"
            Margin="10,5,10,5"
            />
    </Grid>
</Page>
